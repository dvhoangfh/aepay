{
    "name": "dvhoangfh/aepay",
    "description": "Payment for Ae",
    "keywords": [
        "dvhoangfh",
        "aepay"
    ],
    "homepage": "https://github.com/dvhoangfh/aepay",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Hoang Do",
            "email": "dvhoangfh1@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0",
        "irazasyed/telegram-bot-sdk": "^3.9",
        "mautic/api-library": "^3.1"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Dvhoangfh\\Aepay\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dvhoangfh\\Aepay\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dvhoangfh\\Aepay\\AepayServiceProvider"
            ],
            "aliases": {
                "Aepay": "Dvhoangfh\\Aepay\\AepayFacade"
            }
        }
    }
}
